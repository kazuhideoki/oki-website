{
  "name": "gatsby-starter-mate",
  "version": "2.5.3",
  "description": "Gatsby v2 starter to create a top notch portfolio!",
  "main": "index.js",
  "scripts": {
    "get-about": "ts-node --files src/scripts/get-about",
    "check-types": "tsc -p . --noEmit",
    "prebuild": "yarn get-about && yarn check-types",
    "build": "gatsby build",
    "prestart": "yarn get-about",
    "start": "gatsby develop",
    "clean": "rm -rf node_modules && gatsby clean",
    "serve": "yarn build && gatsby serve -p 9000",
    "lint": "eslint ./src",
    "setup": "node ./bin/setup.js",
    "test": "percy exec -t 350 -h cdn-images-1.medium.com -- cypress run",
    "test:open": "percy exec -t 350 -h cdn-images-1.medium.com -- cypress open",
    "test:ci": "DETERMINISTIC=true start-server-and-test 8000",
    "update-media": "cypress run -C ./cypress-media.json",
    "update-media:ci": "DETERMINISTIC=true start-server-and-test 8000 update-media"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmaSuriano/gatsby-starter-mate.git"
  },
  "keywords": [
    "gatsby",
    "starter",
    "contentful",
    "PWA",
    "portfolio"
  ],
  "author": "Ema Suriano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmaSuriano/gatsby-starter-mate/issues"
  },
  "homepage": "https://github.com/EmaSuriano/gatsby-starter-mate#readme",
  "dependencies": {
    "@babel/preset-env": "^7.13.9",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@rebass/preset": "^4.0.5",
    "@tippy.js/react": "^3.1.1",
    "contentful": "^7.15.2",
    "gatsby": "^2.32.9",
    "gatsby-plugin-feed": "^2.13.1",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-offline": "^3.10.2",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-plugin-typescript": "^2.12.1",
    "gatsby-source-contentful": "^3.1.3",
    "gatsby-source-medium": "^2.10.0",
    "gatsby-transformer-remark": "^2.16.1",
    "html-to-text": "^6.0.0",
    "node-fetch": "^2.6.1",
    "prop-types": "^15.6.2",
    "react": "^16.14.0",
    "react-awesome-reveal": "^3.7.1",
    "react-dom": "^16.14.0",
    "react-headroom": "^3.1.0",
    "react-helmet": "^6.1.0",
    "react-jss": "^10.5.1",
    "react-markdown": "^4.3.1",
    "react-responsive": "^8.2.0",
    "react-scroll-section": "^3.0.2",
    "react-text-loop": "^2.0.1",
    "rebass": "^4.0.7",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@percy/cypress": "^2.3.4",
    "@types/react-headroom": "^2.2.1",
    "@types/react-helmet": "^6.1.0",
    "@types/rebass": "^4.0.8",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "axe-core": "^4.1.3",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "contentful-import": "^7.9.40",
    "cypress": "^4.12.1",
    "cypress-axe": "^0.9.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "inquirer": "^7.3.3",
    "polished": "^4.1.1",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.12.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
